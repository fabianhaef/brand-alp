{
  "require": {
    "craftcms/ckeditor": "3.8.1",
    "craftcms/cms": "4.8.10",
    "dodecastudio/craft-blurhash": "^2.0",
    "ether/seo": "4.2.2",
    "nystudio107/craft-typogrify": "4.0.1",
    "nystudio107/craft-vite": "4.0.9",
    "spicyweb/craft-neo": "^3.5.16",
    "verbb/cp-nav": "4.0.12",
    "verbb/expanded-singles": "^2.0.0",
    "verbb/field-manager": "^3.0.5",
    "verbb/hyper": "1.1.16",
    "vlucas/phpdotenv": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "craftcms/plugin-installer": true,
      "treeware/plant": true
    },
    "platform": {
      "php": "8.0.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  ]
}